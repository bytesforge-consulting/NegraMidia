{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/robots.txt",
          "/sitemap.xml",
          "/*.js",
          "/*.css",
          "**/**.css",
          "**/**.js",
          "**/**.ts"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**",
          "https://chronos-common-cdn.azureedge.net/negramidia/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "**/**.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "request api",
      "urls": ["http://localhost.com/posts"],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "timeout": "20s",
        "strategy": "freshness"
      }
    }
  ]
}
